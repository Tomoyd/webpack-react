{
  "extends": "react-app",
  "plugins": ["prettier"],
  "rules": {
    "prefer-template": 0,
    "prettier/prettier": "error",
    "consistent-return": "error",
    "no-unused-vars": "error",
    "prefer-arrow-callback": "error",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ]
  },
  "ignorePatterns": "/node_modules"
}
